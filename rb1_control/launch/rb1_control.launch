<?xml version="1.0"?>
<launch>

  <!-- Load joint controller configurations from YAcd .ML file to parameter server -->
  <rosparam file="$(find rb1_control)/config/rb1_control.yaml" command="load"/>

  <!-- load the controllers -->
  <node name="controller_spawner" pkg="controller_manager" type="spawner" respawn="false"
    output="screen" args="--shutdown-timeout 1
                      joint_read_state_controller 
                      mobile_base_controller
                      torso_slider_controller
                      head_pan_controller
                      head_tilt_controller  
                      arm_j1_position_controller
                      arm_j2_position_controller
                      arm_j3_position_controller
                      "/>

<!-- 
                      arm_j4_position_controller
                      arm_j5_position_controller
                      arm_j6_position_controller
                      finger_j1_position_controller
                      finger_j2_position_controller
                      finger_j3_position_controller
                      arm_controller
                     mico_arm_controller
                      gripper_controller
-->

<!-- 
                      joint_left_velocity_controller
                      joint_right_velocity_controller                      
-->
    
  <node pkg="twist_mux" type="twist_mux" name="twist_mux">
    <rosparam command="load" file="$(find rb1_base_control)/config/twist_mux.yaml" />
    <remap from="cmd_vel_out" to="/rb1/cmd_vel"/>
  </node>


  <!-- once the joint_state is published, run the robot_state_publisher, the namespace has to be adapted --> 
  <!-- convert joint states to TF transforms for rviz, etc -->
  <node name="robot_state_publisher_base" pkg="robot_state_publisher" type="robot_state_publisher"
    respawn="false" output="screen">
  </node>

</launch>

